{
  "name": "bunnymatic-api-js",
  "version": "0.0.1",
  "description": "Javascript needed for the Bunnymatic API app",
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "lint": "npm-run-all eslint-lint prettier-lint",
    "lint-fix": "npm-run-all prettier-fix",
    "eslint-lint": "eslint web/static/**/*.{js,jsx}",
    "prettier-fix": "prettier --write 'web/static/**/*.{js,jsx}' 'web/static/**/*.scss' '**/*.json'",
    "prettier-lint": "prettier --list-different 'web/static/**/*.{js,jsx}' 'web/static/**/*.scss' '**/*.json'",
    "start": "webpack --watch-stdin --progress --color",
    "test": "jest"
  },
  "dependencies": {
    "@uppy/react": "^1.1.0",
    "acorn": "^6.1.1",
    "classnames": "^2.2.6",
    "eslint": "^5.16.0",
    "mini-css-extract-plugin": "^0.7.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uppy": "^1.0.0"
  },
  "peerDependencies": {
    "acorn": "^6.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.0",
    "babel-preset-airbnb": "^3.2.1",
    "core-js": "^3.1.3",
    "css-loader": "^2.1.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fsevents": "^2.0.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier-eslint": "^8.8.2",
    "sass-loader": "^7.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.0.0"
  },
  "browserslist": [
    "defaults",
    "last 2 versions"
  ],
  "resolutions": {
    "webpack/watchpack/chokidar/fsevents": "1.2.4"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "web/static/js"
    ],
    "testURL": "https://www.example.com",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg|ttf|woff|woff2)$": "<rootDir>/__jest_mocks__/fileMock.js",
      "\\.(scss|less)$": "<rootDir>/__jest_mocks__/styleMock.js"
    },
    "roots": [
      "web/static/js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jestConfig.js"
    ]
  },
  "engines": {
    "yarn": "Stop using yarn, please.  Try npm instead"
  }
}
