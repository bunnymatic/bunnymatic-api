{
  "name": "bunnymatic-api-js",
  "version": "0.0.1",
  "description": "Javascript needed for the Bunnymatic API app",
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "lint": "yarn eslint-lint && yarn prettier-lint",
    "lint-fix": "yarn eslint-fix && yarn prettier-fix",
    "eslint-lint": "eslint web/static/**/*.{js,jsx}",
    "eslint-fix": "eslint --fix web/static/**/*.{js,jsx}",
    "prettier-fix": "prettier --write 'web/static/**/*.{js,jsx}' 'web/static/**/*.scss' '**/*.json'",
    "prettier-lint": "prettier --list-different 'web/static/**/*.{js,jsx}' 'web/static/**/*.scss' '**/*.json'",
    "start": "webpack --watch-stdin --progress --color",
    "test": "jest"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "mini-css-extract-plugin": "^0.4.5",
    "ramda": "^0.26.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "uppy": "^0.23.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fsevents": "^1.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^3.0.0",
    "prettier-eslint": "^8.8.2",
    "sass-loader": "^6.0.7",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.0.0"
  },
  "browserslist": [
    "defaults",
    "last 2 versions"
  ],
  "resolutions": {
    "webpack/watchpack/chokidar/fsevents": "1.2.4"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "web/static/js"
    ],
    "testURL": "https://www.example.com",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg|ttf|woff|woff2)$": "<rootDir>/__jest_mocks__/fileMock.js",
      "\\.(scss|less)$": "<rootDir>/__jest_mocks__/styleMock.js"
    },
    "roots": [
      "web/static/js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jestConfig.js"
  },
  "peerDependencies": {
    "eslint": "^5.10.0"
  }
}
