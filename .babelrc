{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%"
      },
      "useBuiltIns": 'usage',
      "corejs": 3
    }],
    "@babel/preset-react",
    "airbnb"
  ],
  "plugins": [
    "@babel/syntax-dynamic-import",
    "@babel/proposal-object-rest-spread",
    ["@babel/proposal-class-properties", { "spec": true }]
  ]
}
